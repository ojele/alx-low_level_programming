#include "main.h"

/**
* largest_number - returns the largest if 3 numbers
* @a: first integer
* @b: second integer
* @c: third integer
* Return: largest number
*/

int largest_number(int a, in b, int c)
{
int largest;

if (a > b)
{
if (a > c)
{
largest = a;
}
else if (b > c)
{
largest = b;
}
else
{
largest = c;
}

return (largest);
}
